<?xml version="1.0"?>
<vbox xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <setting pref="extensions.security-classifier.security-markings" type="string" title="Security Markings" description="A comma-separated list of security markings" />
  <setting pref="extensions.security-classifier.privacy-markings" type="string" title="Privacy Markings" description="A comma-separated list of privacy markings" />
  <setting pref="extensions.security-classifier.warn-external-classified" type="bool" title="Warn on classified messages to external recipients" description="Warn on classified messages to external recipients" />
  <setting pref="extensions.security-classifier.internal-domain" type="string" title="Internal email domain" description="Internal email domain" />
  <setting pref="extensions.security-classifier.logging-enabled" type="bool" title="Enable Debugging" description="Show debugging output in the Error Console" />
